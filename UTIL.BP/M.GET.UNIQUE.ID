SUBROUTINE M.GET.UNIQUE.ID(UNIQUE.ID)
$INCLUDE UTIL.BP UTIL.H
   * CALL M.CRT.ACTION("UNIQIFY",UNIQUE.ID)
   PREFIX = ""
   IF UNIQUE.ID <> "" THEN
      PREFIX = UNIQUE.ID:U_SEP
   END
   UNIQUE.ID = PREFIX:@USERNO:@YEAR4:@MONTH:@DAY:OCONV(@TIME, "MTS"):RND(1000)
   UNIQUE.ID = CONVERT(":", "", UNIQUE.ID)

   RETURN
END
