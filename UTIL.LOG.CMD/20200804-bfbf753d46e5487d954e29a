        <!-- this trick is used to force reload page when navigate browser history in safari
    because safari ignores response headers  -->
<script>
    !function() {
        var ua = navigator.userAgent.toLowerCase();
        if (ua.indexOf('safari') !== -1 &&  ua.indexOf('chrome') === -1) {
            window.addEventListener('pageshow', function(event) {
                if (event.persisted) {
                    window.location.reload()
                }
            });
        }
    }();
</script>        <!-- the size of if is about 250kb and it is not necessary on the page load, that's why we load it in the end -->
