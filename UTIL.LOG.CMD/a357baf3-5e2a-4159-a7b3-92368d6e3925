<script>
    !function() {
        var ua = navigator.userAgent.toLowerCase();
        if (ua.indexOf('safari') !== -1 &&  ua.indexOf('chrome') === -1) {
            window.addEventListener('pageshow', function(event) {
                if (event.persisted) {
                    window.location.reload()
                }
            });
        }
    }();
