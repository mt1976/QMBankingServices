$INSERT BP I_UTIL.H
$INSERT BP I_HTTP.H
$INSERT BP F_WEB.CREDENTIALS.H
$INSERT BP F_WEB.CREDENTIALS.IDX.H

   PN=SYSTEM(45)
   CALL U_INITIALISE(PN)
   CALL U_START(PN)
   CALL U_HEADER(PN)

   CALL W_SERVICE.HOUSEKEEPING_DO("", "", "", responseContent, responseStatus)

   CALL U_CRT.INFO("STATUS", responseStatus)
   CALL U_CRT.INFO("CONTENT", responseContent)

   CALL U_STOP(PN)
   STOP
END
