PROGRAM U_TEST
** INFORMATION ****************************************************************
*   Routine Name : U_TEST5
*           Type : PROGRAM
*         Params : 
*            Loc : BP
** AUDIT **********************************************************************
*   Info Updated : 20210411 at 16.25.52 in MWT-QM-DEV by matttownsend
*                : on silicon.local (Mac)
*******************************************************************************

$INSERT BP I_UTIL.H
$INSERT BP I_HTTP.H
$INSERT BP F_UTIL.TRANSLATE.H

   PN=SYSTEM(45)
   CALL U_INITIALISE(PN)
   CALL U_START(PN)
   CALL U_HEADER(PN)

   PWD = "123456" ; KEY = ""
   BUNDLE = ""
   BUNDLE<1> = "username":W$SEP_DATA:"fred"
   BUNDLE<2> = "password":W$SEP_DATA:"pumpkin"
   BUNDLE = CHANGE(BUNDLE, @AM, W$SEP_LIST)
   CALL U_CRT.INFO("BUNDLE", BUNDLE)
   RESPONSE = ""
   CALL W_BUNDLE.GET(BUNDLE, "username", RESPONSE)
   CALL U_CRT.INFO("username", RESPONSE)
   RESPONSE = ""
   CALL W_BUNDLE.GET(BUNDLE, "password", RESPONSE)
   CALL U_CRT.INFO("password", RESPONSE)

   CALL U_CRT.INFO("EPOCH", EPOCH())

   CALL U_STOP(PN)
   STOP
END
