<?xml version='1.0' encoding='UTF-8'?>
<wctResponse>
  <!-- Request Information -->
  <requestID>{{requestID}}</requestID>
  <requestFileName>{{requestFileName}}</requestFileName>
  <requestFilePath>{{requestFilePath}}</requestFilePath>
  <requestFileType>{{requestFileType}}</requestFileType>
  <requestConsumed>{{requestConsumed}}</requestConsumed>
  <requestAction>{{requestAction}}</requestAction>
  <requestItem>{{requestItem}}</requestItem>
  <requestParameters>{{requestParameters}}</requestParameters>
  <!-- Response Header -->
  <responseID>{{responseID}}</responseID>
  <responseFileName>{{responseFileName}}</responseFileName>
  <responseFilePath>{{responseFilePath}}</responseFilePath>
  <responseFileType>{{responseFileType}}</responseFileType>
  <responseEjected>{{responseEjected}}</responseEjected>



  <!-- Payload from the Request -->
  <requestPayload><![CDATA[{{requestPayload}}]]></requestPayload>
</wctResponse>
