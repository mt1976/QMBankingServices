<NS1:MBT_RES xmlns:NS1="http://dnb.lt/dnb-xst/MBT">
	<NS1:MBT_HEADER>
		<NS1:XREF>{{XREF}}</NS1:XREF>
		<NS1:SOURCE>MB_TODO</NS1:SOURCE>
		<NS1:OPERATION>{{OPERATION}}</NS1:OPERATION>
		<NS1:DESTINATION>{{SOURCE}}</NS1:DESTINATION>
		<NS1:MSGSTAT>SUCCESS</NS1:MSGSTAT>
	</NS1:MBT_HEADER>
	<NS1:MBT_BODY>
		<NS1:FundsCheckResponse>
			<NS1:BIC>{{BIC}}</NS1:BIC>
			<NS1:ACC>{{ACC}}</NS1:ACC>
			<NS1:CCY>{{CCY}}</NS1:CCY>
			<NS1:AMOUNT>{{AMOUNT}}</NS1:AMOUNT>
			<NS1:QUERYTIMESTAMP>{{YYYY}}-{{MM}}-{{DD}}T{{HH}}:{{mm}}:{{SS}}.788332</NS1:QUERYTIMESTAMP>
                        <!-- Result. 1 means OK -->
			<NS1:RESULTCODE>{{RESULTCODE}}</NS1:RESULTCODE>
		</NS1:FundsCheckResponse>
	</NS1:MBT_BODY>
</NS1:MBT_RES>
