* AccuTerm sample program to call NOTEPAD
PRINT 'ENTER FILE':
INPUT FILE
PRINT 'ENTER ID':
INPUT ID
OPEN FILE TO FV ELSE STOP 201,FILE
READ ITEM FROM FV,ID ELSE STOP 202,ID
IF ITEM = "" THEN STOP
CALL DOSSVC('NOTEPAD C:\TEMP\TMP.TXT','C:\TEMP','TMP.TXT',ITEM,'C:\TEMP','TMP.TXT',NEWITEM)
WRITE NEWITEM ON FV,ID
END
