<script>
function openBondFactsheet() {
jQuery('html, body').css('position', 'relative')
var iframe = jQuery('<iframe>', {
class : 'pdf-viewer',
src : '/download/bondfactsheet/XS1112834608ZZGBPUKCP.pdf'
})
jQuery('<div>', { id : 'bond_factsheet_box' }).append( iframe )
.dialog({
modal : true,
close : destroyDialog,
draggable : false,
height : jQuery(window).height() - 32,
width : '90%'
})
jQuery('.ui-widget-overlay').on('click', destroyDialog);
}
function destroyDialog() {
jQuery('html, body').css('position', null)
jQuery('#bond_factsheet_box').empty().dialog('destroy').remove()
}
</script>
</div>
</div>
<script>
jQuery(document).ready(function() {
jQuery('.company_summary_menu .btn-default').click(function(e) {
if (jQuery(e.target).is('a')) {
return true;
}
var link = jQuery('a', this).trigger('click');
var linkUrl = link.attr('href');
var linkTarget = link.attr('target');
if (typeof linkTarget !== typeof undefined && linkTarget !== false) {
window.open(linkUrl,'_blank');
} else {
window.location = linkUrl;
}
return false;
});
});
</script>		<div class="clear"></div>
		
