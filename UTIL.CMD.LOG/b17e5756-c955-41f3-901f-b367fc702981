	<!-- end login container -->
	<script type="text/javascript">
		initProcess();
		
		jQuery(document).ready(function() {
			var loginLayer = jQuery(".login_layer");
			var loginOff = jQuery(".login");
			var loginOn = jQuery('.login_on');
			
			/* open login layer */
			loginOff.on('touchstart click', function()	{
				loginLayer.addClass("active");
				loginOn.show();
				loginOff.hide();
			});
			
			/* close login layer */
			function closeLogin() {
				loginLayer.removeClass("active");
				loginOn.hide();
				loginOff.show();
			}
			
			jQuery(".close_login").on('touchstart click', function() {
				closeLogin();
			});
			
			jQuery('html').on('touchstart click', function() {
				if (loginLayer.hasClass('active')){
					closeLogin();
				}
			});
			
			jQuery('.login_layer, .login, .login_on').on('touchstart click', function(event){
				event.stopPropagation();
			});
		});		  
	</script>

	
