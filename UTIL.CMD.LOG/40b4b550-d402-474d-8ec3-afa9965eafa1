			<!-- External Link: Open dialog and redirect external link -->
		<script type="text/javascript">
		function loadExtLinkDialog(extUrl){
			window.open(extUrl, '_blank');
			jQuery("#dialog-modal").dialog('close');
			return false;
		}

		function loadExtLinkDialogreport(extUrl){
			window.open(extUrl, '_blank');
			jQuery("#dialog-modal-report").dialog('close');
			return false;
		}
			
		jQuery(document).ready(function() {
			jQuery("#dialog-modal").dialog({
				bgiframe:true,
				autoOpen: false,
				height: 250,
				width: 500,
				modal: { backgroundColor: "#000", opacity: 0.5 }
			});
			
			jQuery("#dialog-modal-report").dialog({
				bgiframe:true,
				autoOpen: false,
				height: 300,
				width: 500,
				modal: { backgroundColor: "#000", opacity: 0.5 }
			});
		
		
			jQuery("a[name='openerextlink']").each(function(){
				jQuery(this).on( 'click', function() {
					jQuery("#dialog-modal").dialog('open');
					setTimeout("loadExtLinkDialog('"+this.href+"')", 5000);
					return false;
				});
			});	

			jQuery("a[name='openerextlinkreport']").each(function(){
				jQuery(this).on( 'click', function() {
					jQuery("#dialog-modal-report").dialog('open');
					setTimeout("loadExtLinkDialogreport('"+this.href+"')", 5000);
					return false;
				});
			});	
		});			
		</script>
