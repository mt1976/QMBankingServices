   URI = "https://www.barchart.com/forex/quotes/%5EUSDCAD/forward-rates"
   VERBOSE = @TRUE
   RESULT = ""

   * CALL U_OS.CHROME.FETCH(URI, VERBOSE, RESULT)

   CALL U_CRT.INFO("START", "START")
   CALL U_CRT.RECORD(RESULT)
   CALL U_CRT.INFO("STOP", "STOP")


   CALL U_STOP("")
